{
  "name": "k-heart-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js --config src/ormconfig.ts schema:drop",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js --config src/ormconfig.ts schema:sync",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js --configName src/ormconfig.ts seed",
    "reset": "ts-node ./node_modules/typeorm/cli.js --config src/ormconfig.ts schema:drop && ts-node ./node_modules/typeorm/cli.js --config src/ormconfig.ts schema:sync && ts-node ./node_modules/typeorm-seeding/dist/cli.js --configName src/ormconfig.ts seed && npm run start:dev"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/common": "^8.0.4",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.4",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-express": "^8.0.4",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.0.6",
    "@nestjs/typeorm": "^8.0.1",
    "@types/morgan": "^1.9.3",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "express-session": "^1.17.2",
    "faker": "^4.1.0",
    "handlebars": "^4.7.7",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "nest-morgan": "^2.0.0",
    "nestjs-real-ip": "^2.0.0",
    "nodemailer": "^6.6.3",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "pm2": "^5.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.2",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.4",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "^26.0.24",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.4.0",
    "@types/passport": "^1.0.7",
    "@types/passport-google-oauth20": "^2.0.9",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "run-script-webpack-plugin": "^0.0.11",
    "start-server-nestjs-webpack-plugin": "^2.2.5",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
